{
  "compilerOptions": {
    "allowUnreachableCode": false,
    "alwaysStrict": false,
    "baseUrl": "./src",
    "declaration": false,
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "lib": [
      "es2018",
      "es2016.array.include",
      "dom"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": false,
    "noImplicitAny": true,
    "noImplicitUseStrict": true,
    "outDir": "./dist/out-tsc",
    "paths": {
      "@environments/*": [
          "./environments/*"
      ],
      "@app/*": [ "./app/*" ],
      "@domain/*": [ "./app/domain/*" ],
      "@faq-categories/*": [ "./app/faq-categories/*" ],
      "@faq-items/*": [ "./app/faq-items/*" ],
    },
    "removeComments": true,
    "sourceMap": true,
    "target": "es2015"
  },
  "typeRoots": [
    "node_modules/@types"
  ],
  "types": [
    "node",
    "tinymce"
  ],
  "compileOnSave": false,
  "atom": { "rewriteTsconfig": false },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
