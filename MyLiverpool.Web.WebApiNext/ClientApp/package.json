{
  "name": "asp.net",
  "version": "1.0.0",
  "scripts": {
    "run": "ng serve --hmr false --proxy-config proxy.conf.json",
    "run2": "ng serve  --proxy-config proxy.conf.json",
    "run3": "ng serve  --proxy-config proxy.conf2.json",
    "build": "ng build --prod",
    "build-nohash": "ng build --prod --build-optimizer --output-hashing none",
