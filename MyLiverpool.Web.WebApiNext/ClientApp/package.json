{
  "name": "asp.net",
  "version": "1.0.0",
  "scripts": {
    "run": "ng serve --hmr false --proxy-config proxy.conf.json",
    "run2": "ng serve  --proxy-config proxy.conf.json",
    "build": "ng build --prod --build-optimizer",
    "build-ssr": "ng build --prod --project=ssr",
    "build-nohash": "ng build --prod --build-optimizer --output-hashing none",
    "build:ssr": "npm run build-ssr",
    "ng": "ng",
    "start": "ng serve",
    "lint": "ng lint",
    "analyze": "npm run build -- --stats-json && webpack-bundle-analyzer dist/aspnetcorespa/stats.json",
    "source-map": "npm run build -- --sourceMap=true --outputHashing=none && source-map-explorer dist/aspnetcorespa/main.js dist/aspnetcorespa/main.js.map"
  },
  "dependencies": {
    "@angular/animations": "8.0.0-beta.13",
    "@angular/cdk": "8.0.0-beta.2",
    "@angular/common": "8.0.0-beta.13",
    "@angular/compiler": "8.0.0-beta.13",
    "@angular/compiler-cli": "8.0.0-beta.13",
    "@angular/core": "8.0.0-beta.13",
    "@angular/forms": "8.0.0-beta.13",
    "@angular/material": "8.0.0-beta.2",
    "@angular/platform-browser": "8.0.0-beta.13",
    "@angular/platform-browser-dynamic": "8.0.0-beta.13",
    "@angular/platform-server": "8.0.0-beta.13",
    "@angular/router": "8.0.0-beta.13",
    "@angular/service-worker": "8.0.0-beta.13",
    "@aspnet/signalr": "1.0.4",
    "@aspnet/signalr-protocol-msgpack": "1.0.4",
    "@nguniversal/aspnetcore-engine": "7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "7.1.1",
    "angular2-recaptcha": "1.1.0",
    "aspnet-prerendering": "3.0.1",
    "aspnet-webpack": "3.0.0",
    "core-js": "2.6.5",
    "hammerjs": "2.0.8",
    "ngx-image-cropper": "1.3.9",
    "ngx-pagination": "3.2.1",
    "node-sass": "4.11.0",
    "preboot": "7.0.0",
    "rxjs": "6.4.0",
    "swagger-ui": "3.22.1",
    "zone.js": "0.9.0"
  },
  "devDependencies": {
    "@angular/cli": "8.0.0-beta.16",
    "@angular-devkit/build-angular": "0.800.0-beta.16",
    "@angular/pwa": "0.800.0-beta.16",
    "typescript": "3.4.4",
    "ts-node": "8.0.3",
    "tslint": "5.15.0",
    "codelyzer": "5.0.0",
    "tinymce": "4.9.3",
    "@types/node": "11.13.6",
    "@types/tinymce": "4.5.20",
    "webpack-bundle-analyzer": "3.3.2",
    "acorn": "6.1.1",
    "source-map-explorer": "1.8.0"
  },
