{
  "name": "asp.net",
  "version": "1.0.0",
  "main": "wwwroot/index.html",
  "scripts": {
    "ngc": "ngc -p ./tsconfig-prod.json",
    "startWebpackDevServer": "concurrently \"webpack-dev-server --inline --progress --port 1669\" \"dotnet run\" ",
    "start": "webpack-dev-server --inline --progress --port 1669",
    "webpack-dev": "set NODE_ENV=development&& webpack",
    "webpack-prod": "set NODE_ENV=production&& webpack",
    "build": "npm run webpack-dev",
    "buildProduction": "npm run ngc && npm run webpack-prod",
    "lint": "tslint ./angular2app/**/*.ts -t verbose",
    "rebuilt": "npm rebuild node-sass",

    "devserver:jit": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.dev.js  --profile --progress --inline --hot --compress --content-base wwwroot"
  },
