{
  "name": "asp.net",
  "version": "1.0.0",
  "main": "wwwroot/index.html",
  "scripts": {
    "ngc": "ngc -p ./tsconfig-prod.json",
    "startWebpackDevServer": "concurrently \"webpack-dev-server --inline --progress --port 1669\" \"dotnet run\" ",
    "start": "webpack-dev-server --inline --progress --port 1669",
    "before": "webpack --config config/webpack.config.vendor.js",
    "webpack-dev": "set NODE_ENV=development&& webpack",
    "webpack-prod": "set NODE_ENV=production&& webpack",
    "build": "npm run webpack-dev",
    "buildProduction": "npm run ngc && npm run webpack-prod",
    "lint": "tslint ./angular2app/**/*.ts -t verbose",
    "rebuilt": "npm rebuild node-sass",
    "devserver:jit": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.dev.js  --profile --progress --inline --hot --compress --content-base wwwroot"
  },
  "dependencies": {
    "@angular/common": "2.2.1",
    "@angular/compiler": "2.2.1",
    "@angular/compiler-cli": "2.2.1",
    "@angular/core": "2.2.1",
    "@angular/forms": "2.2.1",
    "@angular/http": "2.2.1",
    "@angular/material": "2.0.0-alpha.10",
    "@angular/platform-browser": "2.2.1",
    "@angular/platform-browser-dynamic": "2.2.1",
    "@angular/platform-server": "2.2.1",
    "@angular/router": "3.2.1",
    "core-js": "2.4.1",
    "ie-shim": "0.1.0",
    "jquery": "2.2.4",
    "moment": "2.16.0",
    "ng2-auto-complete": "0.7.4",
    "ng2-bootstrap": "1.1.16",
    "ng2-file-upload": "1.1.4-2",
    "reflect-metadata": "0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "0.6.26"
  },
  "devDependencies": {
    "@angularclass/conventions-loader": "^1.0.2",
    "@angularclass/hmr": "~1.2.0",
    "@angularclass/hmr-loader": "~3.0.2",
    "@angularclass/request-idle-callback": "^1.0.7",
    "@angularclass/webpack-toolkit": "^1.3.3",
    "@types/hammerjs": "2.0.33",
    "@types/node": "^6.0.48",
    "@types/source-map": "0.1.29",
    "@types/uglify-js": "2.6.28",
    "@types/webpack": "1.12.35",
    "angular2-platform-node": "~2.0.11",
    "angular2-template-loader": "^0.6.0",
    "angular2-universal": "~2.0.11",
    "angular2-universal-polyfills": "~2.0.11",
    "aspnet-prerendering": "^1.0.7",
    "aspnet-webpack": "^1.0.24",
    "awesome-typescript-loader": "2.2.4",
    "bootstrap": "3.3.7",
    "codelyzer": "0.0.28",
    "css": "^2.2.1",
    "css-loader": "0.26.0",
    "es6-shim": "^0.35.1",
    "event-source-polyfill": "^0.0.7",
    "exports-loader": "0.6.3",
    "expose-loader": "0.7.1",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "isomorphic-fetch": "2.2.1",
    "preboot": "^4.5.2",
    "raw-loader": "0.5.1",
    "style-loader": "0.13.1",
    "swagger-ui": "2.2.6",
    "to-string-loader": "1.1.5",
    "ts-loader": "1.2.2",
    "typescript": "2.1.1",
    "url-loader": "0.5.7",
    "webpack": "2.1.0-beta.27",
    "webpack-hot-middleware": "2.13.2",
